package br.ufjf.dcc.poo.exercicios;

import javax.swing.JOptionPane;

public class ConsumoCombustivel {

	public static void main(String[] args) {
		int kmDirigidos, tanqueCheio;
		kmDirigidos = Integer.parseInt(JOptionPane.showInputDialog("Km dirigidos", null));
		tanqueCheio = Integer.parseInt(JOptionPane.showInputDialog("Litros no tanque", null));
		
		float resultado = (float)kmDirigidos/tanqueCheio;
		
		JOptionPane.showMessageDialog(null, resultado);
		
	}

}
